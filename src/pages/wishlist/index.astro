---
import WishlistPage from "@components/wishlist/WishlistPage.astro";
import { getWishlistItems, categories } from "@lib/wishlist";

export const prerender = false;

Astro.response.headers.set("CDN-Cache-Control", "no-store");

const items = await getWishlistItems();
---

<WishlistPage items={items} categories={categories} currentCategory="all" />
